@model NowDelivary.Models.Area

@{
    ViewData["Title"] = "EditArea";
}

<h1>EditArea</h1>
<div class="container">
    <div class="row">
        <div class="col-md-4">
            <form asp-action="EditArea" method="post">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="ID" />
                <div class="form-group">
                    <label asp-for="AreaName" class="control-label"></label>
                    <input asp-for="AreaName" class="form-control" />
                    <span asp-validation-for="AreaName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>


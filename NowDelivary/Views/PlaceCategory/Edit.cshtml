@model NowDelivary.Models.PlaceCategory

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Module</h1>


<hr />
<div class="container">
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Edit" method="post">
                <input type="hidden" asp-for="ID" />
                <div class="form-group">
                    <label asp-for="PlaceCategoryName" class="control-label"></label>
                    <input asp-for="PlaceCategoryName" class="form-control" />
                    <span asp-validation-for="PlaceCategoryName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>



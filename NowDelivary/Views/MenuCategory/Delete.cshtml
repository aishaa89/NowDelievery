@model MenuCategory
@{
    ViewData["Title"] = "Delete";
}

<Div class="col-md-4">
    <form asp-action="Delete" asp-controller="MenuCategory" method="post">
        <div class="form-group">
            <label asp-for="@Model.MenuCategoryType">Menu item</label>
            <input asp-for="@Model.MenuCategoryType" placeholder="Enter Menu item" />
        </div>
        <div class="form-group">
            <label asp-for="@Model.PlaceID">item Price</label>
            <input asp-for="@Model.PlaceID" placeholder="Enter item Price" />
        </div>
       

        <button class="btn btn-block btn-success" type="submit">Save</button>
    </form>
</Div>


